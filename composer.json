{
  "name": "oliverschloebe/oauth2-steadyhq",
  "description": "Steady OAuth 2.0 package for the PHP League's OAuth 2.0 Client",
  "license": "MIT",
  "authors": [
    {
      "name": "Oliver Schl√∂be",
      "email": "github@schloebe.de",
      "role": "Developer"
    }
  ],
  "keywords": [
    "php",
    "api",
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authorisation",
    "idp",
    "identity",
    "sso",
    "single sign on",
    "steadyhq"
  ],
  "support": {
    "issues": "https://github.com/oliverschloebe/oauth2-steadyhq/issues",
